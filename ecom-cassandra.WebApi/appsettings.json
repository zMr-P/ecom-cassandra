{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ExternalServices": {
    "Cassandra": {
      "Username": "your_username",
      "Password": "your_password",
      "Host": "cassandra_host",
      "Port": 9042,
      "Keyspace": "your_keyspace"
    }
  },
  "Jwt": {
    "Issuer": "your_issuer",
    "Audience": "your_audience",
    "SecretKey": "your_secret_key",
    "TokenExpirationMinutes": 120
  }
}
